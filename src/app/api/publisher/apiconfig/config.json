{
  "baseUrl": "http://localhost:8083/api",
  "useEnvBaseUrl": true,
  "timeout": 3000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/biz/user/register",
      "login": "/biz/user/login",
      "logout": "/biz/user/logout",
      "resetpwd": "/biz/user/reset-password",
      "refreshToken": "/biz/user/refresh",
      "userlist": "/biz/user/list"
    },
    "user": {
      "getuserinfo": "/biz/user/profile",
      "changepwd": "/biz/user/change-password",
      "getbizuserlist": "/biz/user/main-accounts/list"
    },
    "wallet": {
      "walletinfo": "/wallet/info",
      "topup": "/biz/wallet/topup",
      "withdraw": "/biz/wallet/withdraw"
    },
    "task": {
      "publish": "/tasks/publish",
      "mypublished": "/tasks/my-published",
      "pendingverify": "/tasks/pending-verify",
      "reviewtask": "/tasks/accepted/{subTaskId}/verify",
      "taskcount": "/tasks/stats"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}