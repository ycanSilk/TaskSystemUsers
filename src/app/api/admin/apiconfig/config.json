{
  "baseUrl": "http://localhost:8083/api",
  "timeout": 5000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/sys/user/register",
      "login": "/sys/user/login",
      "logout": "/sys/user/logout",
      "resetpwd": "/sys/user/reset-password",
      "refreshToken": "/sys/user/refresh",
      "userlist": "/sys/user/list",
      "forgotpwd": "/sys/user/forgot-password",
      "changepwd": "/sys/user/change-password",
      "userprofile": "/sys/user/profile",
      "createuser": "/sys/user"
    },
    "role": {
      "getrolelist": "/sys/role/list",
      "createrole": "/sys/role/"
    },
    "wallet": {
      "recharge": "/wallet/admin/recharge"
    },
    "menu": {
      "createmenu": "/sys/menu/",
      "getmenutree": "/sys/menu/tree",
      "getchildrenmenulist": "/sys/menu/children"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}