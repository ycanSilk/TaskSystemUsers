{
  "baseUrl": "http://localhost:8083/api",
  "useEnvBaseUrl": true,
  "timeout": 5000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/ind/user/register",
      "login": "/ind/user/login",
      "logout": "/ind/user/logout"
    },
    "user": {
      "refreshToken": "/ind/user/refresh",
      "getinduserlist": "/ind/user/list",
      "resetpwd": "/ind/user/reset-password",
      "forgotpwd": "/ind/user/forgot-password",
      "changepwd": "/ind/user/change-password",
      "userprofile": "/ind/user/profile"
    },
    "task": {
      "accepttask": "/tasks/{taskId}/accept",
      "myacceptedtasks": "/tasks/my-accepted",
      "submittask": "/tasks/accepted/{subTaskId}/submit"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}